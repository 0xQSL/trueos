{
  "ports":{
	"type":"git",
	"url":"https://github.com/trueos/trueos-ports",
	"branch":"trueos-master",
	"make.conf":{
		"default": [
			"devel_git_UNSET=PERL CVS P4 GITWEB"
		],
		"WITH_XTRA_PKGS":[
			"archivers_cabextract_UNSET=docs"
		]
	}
  },
  "package-all":false,
  "packages":{
	"default":[
		"devel/git",
		"sysutils/ipmitool",
		"sysutils/dmidecode",
		"sysutils/tmux"
	],
	"WITH_XTRA_PACKAGES": [
		"archivers/cabextract"
	]
  },
  "essential-packages":{
	"default":[
		"devel/git",
		"sysutils/ipmitool",
		"sysutils/dmidecode",
		"sysutils/tmux"
	],
	"WITH_XTRA_PACKAGES": [
		"archivers/cabextract"
	]
  },
  "iso-install-packages":{
	"default": [
		"archivers/cabextract",
		"devel/git",
		"sysutils/ipmitool",
		"sysutils/dmidecode",
		"sysutils/tmux"
	],
	"WITH_XTRA_PACKAGES": [
		"archivers/cabextract"
	]
  },
  "dist-packages":{
	"default": [
		"devel/git",
		"sysutils/ipmitool",
		"sysutils/dmidecode",
		"sysutils/tmux"
	],
	"WITH_XTRA_PACKAGES": [
		"archivers/cabextract"
	]
  },
  "auto-install-packages":{
	"default": [
		"devel/git",
		"sysutils/ipmitool",
		"sysutils/dmidecode",
		"sysutils/tmux"
	],
	"WITH_XTRA_PACKAGES": [
		"archivers/cabextract"
	]
  },
  "iso-overlay":{
	"type":"git",
	"url":"https://github.com/trueos/iso-overlay",
	"branch":"master"
  },
  "iso-file-name":"TrueOS-x64-%%TRUEOS_VERSION%%-%%GITHASH%%-%%DATE%%",
  "install-script":"",
  "auto-install-script":"",
  "post-install-commands": [
	  {
		"chroot":true,
		"command":"touch /root/inside-chroot"
	  },
	  {
		"chroot":false,
		"command":"true /root/outside-chroot"
	  },
	  {
		"chroot":false,
		"command":"rm /root/outside-chroot"
	  },
	  {
		"chroot":true,
		"command":"rm /root/inside-chroot"
	  }
  ],
  "base-packages": {
	  "name-prefix":"TrueOS",
	  "depends": {
		  "runtime": {
			"uclcmd": {
				"origin": "devel/uclcmd",
				"version": ">0"
			}
		  },
		 "runtime-development": {
			"llvm60": {
				"origin": "devel/llvm60",
				"version": ">0"
			},
			"go": {
				"origin": "lang/go",
				"version": ">0"
			}
		  }
	  }
  },
